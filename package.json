{
  "name": "my-rsa-implementation",
  "version": "1.0.0",
  "description": "Rsa implementation in typescript",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "scripts": {
    "test": "ts-node test.ts",
    "build:esm": "tsc --target es2020 --module es2020 --declaration --outDir dist/esm src/index.ts",
    "build:cjs": "tsc --target es2020 --module commonjs --declaration --outDir dist/cjs src/index.ts",
    "build": "npm run build:esm && npm run build:cjs"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "author": "Mario",
  "license": "ISC",
  "devDependencies": {
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "bigint-conversion": "^2.4.3",
    "bigint-crypto-utils": "^3.3.0"
  }
}
